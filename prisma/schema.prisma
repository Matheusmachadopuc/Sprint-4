// This file was generated by Prisma (https://www.prisma.io/)
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  name String
  email String @unique
  password String
  level Int
  profile_img String//base64
  createdAt DateTime @default(now())
  deletedAt DateTime? 
}

model Room {
  id String @id @default(uuid())
  description String
  acessLevel Int
}
